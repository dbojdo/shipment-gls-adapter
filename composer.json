{
    "name" : "webit/shipment-gls-adapter",
    "type" : "library",
    "description": "Web-IT Shipment GLS Adapter",
    "keywords": [
        "web-it",
        "shipment",
        "bundle",
        "delivery",
        "parcel",
        "dispatch",
        "gls"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "webit/shipment": "^2.0.0|^3.0.0",
        "webit/gls-ade": "^2.0.0",
        "webit/gls-tracking": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Shipment\\GlsAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\Shipment\\GlsAdapter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
